&i2c4 {
    ts@01 {
		compatible = "ct,vtl_ts";
		reg = <0x01>;
		screen_max_x = <1536>;
		screen_max_y = <2048>;
		irq_gpio_number = <&gpio7 GPIO_A6 IRQ_TYPE_LEVEL_LOW>;
		rst_gpio_number = <&gpio7 GPIO_A5 GPIO_ACTIVE_HIGH>;
        x_mul = <2>;
        x_reverse = <0>;
        xy_swap = <1>;
        y_mul = <2>;
        y_reverse = <0>;
	};
    ts@02 {
        bin_ver = <0x00>;
        compatible = "goodix,gt9xx_ts";
        reg = <0x14>;
        screen_max_x = <1536>;
        screen_max_y = <2048>;
        id_gpio_number = <&gpio7 GPIO_A4 IRQ_TYPE_LEVEL_LOW>;
        irq_gpio_number = <&gpio7 GPIO_A6 IRQ_TYPE_LEVEL_LOW>;
        rst_gpio_number = <&gpio7 GPIO_A5 GPIO_ACTIVE_HIGH>;
        pinctrl-0 = <0xc4>;
        pinctrl-names = "default";
        x_mul = <2>;
        x_reverse = <0>;
        xy_swap = <1>;
        y_mul = <2>;
        y_reverse = <0>;
    };
};
&adc {
	status = "okay";

	key {
		compatible = "rockchip,key";
        io-channels = <&adc 1>;
